<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/MenuFragment">

    <fragment
        android:id="@+id/MenuFragment"
        android:name="rpt.tool.badpixelsearch.ui.menu.MenuFragment"
        android:label="BadPixelSearchFragment"
        tools:layout="@layout/fragment_menu">
        <action
            android:id="@+id/action_menuFragment_to_settingsFragment"
            app:destination="@id/SettingsFragment" />
        <action
            android:id="@+id/action_menuFragment_to_faqFragment"
            app:destination="@id/FaqFragment" />
        <action
            android:id="@+id/action_menuFragment_to_screenInfoFragment"
            app:destination="@id/ScreenInfoFragment" />
        <action
            android:id="@+id/action_menuFragment_to_deviceInfoFragment"
            app:destination="@id/DeviceInfoFragment" />
        <action
            android:id="@+id/action_menuFragment_to_sensorInfoFragment"
            app:destination="@id/SensorInfoFragment" />
        <action
            android:id="@+id/action_menuFragment_to_otherAppsFragment"
            app:destination="@id/OtherAppsFragment" />
        <action
            android:id="@+id/action_menuFragment_to_colorTestMenuFragment"
            app:destination="@id/ColorTestsMenuFragment" />
        <action
            android:id="@+id/action_menuFragment_to_animationTestMenuFragment"
            app:destination="@id/AnimationTestMenuFragment" />
        <action
            android:id="@+id/action_menuFragment_to_systemFontMenuFragment"
            app:destination="@id/SystemFontMenuFragment" />
        <action
            android:id="@+id/action_menuFragment_to_cameraTestMenuFragment"
            app:destination="@id/CameraTestMenuFragment" />
        <action
            android:id="@+id/action_menuFragment_to_fixTestMenuFragment"
            app:destination="@id/FixTestMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/SettingsFragment"
        android:name="rpt.tool.badpixelsearch.ui.settings.SettingsFragments"
        android:label="SettingsFragment"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment_to_menuFragment"
            app:destination="@id/MenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/FaqFragment"
        android:name="rpt.tool.badpixelsearch.ui.faq.FaqFragment"
        android:label="FaqFragment"
        tools:layout="@layout/fragment_faq">
        <action
            android:id="@+id/action_faqFragment_to_menuFragment"
            app:destination="@id/MenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/ScreenInfoFragment"
        android:name="rpt.tool.badpixelsearch.ui.screen.ScreenInfoFragment"
        android:label="ScreenInfoFragment"
        tools:layout="@layout/fragment_screen_info">
        <action
            android:id="@+id/action_screenInfoFragment_to_menuFragment"
            app:destination="@id/MenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/DeviceInfoFragment"
        android:name="rpt.tool.badpixelsearch.ui.device.DeviceInfoFragment"
        android:label="ScreenInfoFragment"
        tools:layout="@layout/fragment_device_info">
        <action
            android:id="@+id/action_deviceInfoFragment_to_menuFragment"
            app:destination="@id/MenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/SensorInfoFragment"
        android:name="rpt.tool.badpixelsearch.ui.sensor.SensorInfoFragment"
        android:label="SensorInfoFragment"
        tools:layout="@layout/fragment_sensor_info">
        <action
            android:id="@+id/action_sensorInfoFragment_to_menuFragment"
            app:destination="@id/MenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/OtherAppsFragment"
        android:name="rpt.tool.badpixelsearch.ui.other.OtherAppsFragment"
        android:label="OtherAppsFragment"
        tools:layout="@layout/fragment_other_apps">
        <action
            android:id="@+id/action_otherAppsFragment_to_menuFragment"
            app:destination="@id/MenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/ColorTestsMenuFragment"
        android:name="rpt.tool.badpixelsearch.ui.color.menu.ColorTestMenuFragment"
        android:label="ColorTestsMenuFragment"
        tools:layout="@layout/fragment_menu_color_test">
        <action
            android:id="@+id/action_colorTestsMenuFragment_to_menuFragment"
            app:destination="@id/MenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/AnimationTestMenuFragment"
        android:name="rpt.tool.badpixelsearch.ui.animation.menu.AnimationMenuFragment"
        android:label="AnimationTestMenuFragment"
        tools:layout="@layout/fragment_animation_menu">
        <action
            android:id="@+id/action_animationMenuFragment_to_menuFragment"
            app:destination="@id/MenuFragment" />
        <action
            android:id="@+id/action_animationMenuFragment_to_movingBarsFragment"
            app:destination="@id/MovingBarFragment" />
        <action
            android:id="@+id/action_animationMenuFragment_to_rotationFragment"
            app:destination="@id/RotationFragment" />
    </fragment>
    <fragment
        android:id="@+id/SystemFontMenuFragment"
        android:name="rpt.tool.badpixelsearch.ui.fonts.menu.SystemFontMenuFragment"
        android:label="SystemFontMenuFragment"
        tools:layout="@layout/fragment_system_font_menu">
        <action
            android:id="@+id/action_systemFontMenuFragment_to_menuFragment"
            app:destination="@id/MenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/MovingBarFragment"
        android:name="rpt.tool.badpixelsearch.ui.animation.bars.MovingBarsFragment"
        android:label="MovingBarFragment"
        tools:layout="@layout/fragment_animation_bars">
        <action
            android:id="@+id/action_movingBarFragment_to_animationMenuFragment"
            app:destination="@id/AnimationTestMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/RotationFragment"
        android:name="rpt.tool.badpixelsearch.ui.animation.rotation.RotationFragment"
        android:label="RotationFragment"
        tools:layout="@layout/fragment_rotation">
        <action
            android:id="@+id/action_rotationBarFragment_to_animationMenuFragment"
            app:destination="@id/AnimationTestMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/CameraTestMenuFragment"
        android:name="rpt.tool.badpixelsearch.ui.camera.menu.CameraMenuFragment"
        android:label="CameraTestMenu"
        tools:layout="@layout/fragment_menu_camera">
    </fragment>
    <fragment
        android:id="@+id/FixTestMenuFragment"
        android:name="rpt.tool.badpixelsearch.ui.fix.menu.FixPixelMenuFragment"
        android:label="FixTestMenuFragment"
        tools:layout="@layout/fragment_fix_pixels_menu">
    </fragment>

</navigation>